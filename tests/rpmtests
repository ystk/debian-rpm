    (('a', '>=', '1.2'),	('a', '<>', '1.2-1'),	1),

    (('a', '<=', '1.2'),	('a', '=', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '>=', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '<=', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '<', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '>', '1.2-1'),	1),
    (('a', '<=', '1.2'),	('a', '<>', '1.2-1'),	1),

    (('a', '<>', '1.2'),	('a', '=', '1.2-1'),	0),
