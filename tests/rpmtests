#AT_START_174
at_fn_group_banner 174 'rpmpython.at:35' \
  "basic rpmio" "                                    " 13
at_xfail=no
(
  $as_echo "174. $at_setup_line: testing $at_desc ..."
  $at_traceon



rm -rf "${abs_builddir}"/testing`rpm --eval '%_dbpath'`/*


