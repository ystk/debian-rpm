    (('a', '=', '1.2'),		('a', '=', '1:1.2'),	0),
    (('a', '=', '1.2'),		('a', '>=', '1:1.2'),	0),
    (('a', '=', '1.2'),		('a', '<=', '1:1.2'),	1),
    (('a', '=', '1.2'),		('a', '<', '1:1.2'),	1),
    (('a', '=', '1.2'),		('a', '>', '1:1.2'),	0),
    (('a', '=', '1.2'),		('a', '<>', '1:1.2'),	1),

    (('a', '=', '2:1.2'),	('a', '=', '2:1.2'),	1),
    (('a', '=', '2:1.2'),	('a', '>=', '2:1.2'),	1),
    (('a', '=', '2:1.2'),	('a', '<=', '2:1.2'),	1),
    (('a', '=', '2:1.2'),	('a', '<', '2:1.2'),	0),
    (('a', '=', '2:1.2'),	('a', '>', '2:1.2'),	0),
    (('a', '=', '2:1.2'),	('a', '<>', '2:1.2'),	0),

    # Simple, obvious version comparisons with provide ranges
    (('a', '>', '1.2'),		('a', '=', '1.2'),	0),
    (('a', '>', '1.2'),		('a', '>=', '1.2'),	1),
    (('a', '>', '1.2'),		('a', '<=', '1.2'),	0),
